<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DoubleStateView">
        <attr name="double_state"
              format="enum">
            <enum name="state_on"
                  value="1"/>
            <enum name="state_off"
                  value="-1"/>
        </attr>
        <attr name="double_trans_duration" format="integer"/>
    </declare-styleable>


    <declare-styleable name="MultipleStateView">
        <attr name="msv_transDuration" format="integer"/>
        <attr name="msv_select" format="integer|string"/>
        <attr name="msv_first_enter_anim" format="boolean"/>
        <attr name="msv_measureMode" format="enum">
            <enum name="FOLLOW_PARENT" value="1"/>
            <enum name="FOLLOW_CHILD" value="2"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="MultipleStateView_Layout">
        <attr name="layout_regName" format="string"/>
    </declare-styleable>
</resources>